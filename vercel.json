{
  "buildCommand": "bash build.sh",
  "outputDirectory": "laravel/public",
  "env": {
    "APP_NAME": "Diamond Academy",
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "LOG_CHANNEL": "stack"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/laravel/public/index.php"
    }
  ]
}
